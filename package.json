{
    "name": "proxemics",
    "version": "0.0.5",
    "description": "Javascript library for Progressive Hover Effects.",
    "main": "./lib/index.js",
    "scripts": {
        "clean": "rimraf lib",
        "lint": "eslint src",
        "build": "cross-env BABEL_ENV=production babel src --out-dir lib && yarn demo",
        "prepublish": "npm run clean && npm run lint && npm run build && yarn demo",
        "demo": "webpack --config webpack.config.demo.js --mode production"
    },
    "files": [
        "lib",
        "src"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pokkur/proxemics.git"
    },
    "keywords": [
        "npm",
        "module",
        "node",
        "javascript"
    ],
    "author": "Pokkur",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/pokkur/proxemics.git/issues"
    },
    "homepage": "https://github.com/pokkur/proxemics.git#readme",
    "devDependencies": {
        "babel-eslint": "^8.2.1",
        "chai": "^4.1.2",
        "cross-env": "^5.1.3",
        "eslint": "^5.4.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jsx-a11y": "^6.0.2",
        "eslint-plugin-react": "^7.4.0",
        "istanbul": "^1.0.0-alpha",
        "mocha": "^5.0.0",
        "rimraf": "^2.6.2",
        "webpack-cli": "^3.1.0",
        "webpack-dev-server": "^3.1.5"
    },
    "dependencies": {
        "@babel/core": "^7.0.0-rc.2",
        "@babel/polyfill": "^7.0.0-rc.2",
        "@babel/preset-env": "^7.0.0-rc.2",
        "babel-loader": "^8.0.0-beta.6",
        "webpack": "^4.16.3"
    },
    "browserslist": [
        "> 1% in JP",
        "ie 11",
        "android > 4.0"
    ],
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "useBuiltIns": "entry"
                }
            ]
        ]
    }
}
